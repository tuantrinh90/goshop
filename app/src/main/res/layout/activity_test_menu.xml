<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <include
        layout="@layout/toolbar_img_text_img_close"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
    android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_login"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/login"/>

            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_send_confirmation_link"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/send_confirmation_link"/>
            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_complement_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/complement_email"/>
            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_my_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/my_address_book"/>

            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_editprofile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/edit_profile"/>

            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_my_points"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/my_point"/>

            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_select_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/select_address"/>

            <com.goshop.app.common.view.CustomTextView
                android:id="@+id/btn_test_paymentstatus"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="@dimen/size_16"
                android:textColor="@color/color_text_black"
                android:background="@drawable/bg_white_top_bottom_line"
                android:textSize="@dimen/size_font_14"
                android:text="@string/payment_status"/>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
